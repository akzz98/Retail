<form asp-action="Register" method="post">
    <div>
        <label>Username</label>
        <input name="username" />
    </div>
    <div>
        <label>Email</label>
        <input name="email" />
    </div>
    <div>
        <label>Password</label>
        <input name="password" type="password" />
    </div>
    <button type="submit">Register</button>
</form>